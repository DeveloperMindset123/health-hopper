{
  "version": 2,
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" },
    { "src": "frontend", "use": "@vercel/next" }
  ],
  "routes": [
    { "src": "/api/:path*", "dest": "/api/index.py" },
    { "src": "/(.*)", "dest": "frontend/build/$1" }
  ],
  "env": {
    "APP_MODULE": "main:app"
  }
}
